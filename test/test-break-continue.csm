int x;
for (x = 0; x < 10; x = x + 1) {
        while (x < 5) {
                x = x + 1;
                if (x > 3) {
                        break;
                }
        }
        print(x);
        if (x < 7) {
                x = 9;
                continue;
        }
        print(x);
        if(x > 8)
                break;
        print(x);
}
print(x);