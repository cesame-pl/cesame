println("Integer greater than 5 and less than 10: ");

for (int i = 0; i < 10; i++) {
    if (i <= 5) {
        continue;
    }

    println(i);
}

println("Positive Integer less than or equal to 20 divisible by 5");
int i = 0; 
while (i < 20) {
    i++;

    if (i % 5 != 0) {
        continue;
    }

    println(i);
}