// 1d array
Array<String> a;
a = ["Hello", "Nice", "to", "See", "You"];
for (int i = 0; i < 5; i++) {
    print(a[i]);
}
delete a;

// 2d array
Array<Array<int>> b = [[-1, -2], [-3, -4], [-5, -6]];
for (int i = 0; i < 3; i++) {
    for (int j = 0; j < 2; j++) {
        print(b[i][j]);
    }
}
delete b;

// 3d array
Array<Array<Array<int>>> c = 
    [
        [[1], [2]], 
        [[3], [4]], 
        [[5], [6]]
    ];
for (int i = 0; i < 3; i++) {
    for (int j = 0; j < 2; j++) {
        c[i][j][0] = c[i][j][0] * 2;
        print(c[i][j][0]);
    }
}
delete c;

